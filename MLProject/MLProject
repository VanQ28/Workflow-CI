name: Amazon_Sales_Project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      train_path: {type: string, default: "Amazon_Preprocessing/amazon_train.csv"}
      test_path: {type: string, default: "Amazon_Preprocessing/amazon_test.csv"}
      experiment_name: {type: string, default: "Amazon_Sales"}
      run_name: {type: string, default: "Random Forest Final Advanced"}
    command: "python Modelling.py --train_path {train_path} --test_path {test_path} --experiment_name {experiment_name} --run_name {run_name}"
