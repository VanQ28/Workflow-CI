name: Amazon_Sales_Project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      train_path: {type: string, default: "Amazon_Preprocessing/amazon_train.csv"}
      test_path: {type: string, default: "Amazon_Preprocessing/amazon_test.csv"}
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
    command: "python modelling.py --train_path {train_path} --test_path {test_path} --n_estimators {n_estimators} --max_depth {max_depth}"
